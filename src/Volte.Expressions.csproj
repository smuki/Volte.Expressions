<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <Description>
      Provides infrastructure for registering and executing workflow expressions.
      Out of the box, it provides a DelegateExpression and LiteralExpression.
    </Description>
  </PropertyGroup>

  <PropertyGroup>
    <NoWarn>1701;1702;1591;1600;1606;1028;1128</NoWarn>
    <ImplicitUsings>enable</ImplicitUsings>
  </PropertyGroup>
      <ItemGroup>
        <PackageReference Include="Microsoft.Extensions.DependencyInjection.Abstractions" Version="8.0.0" />
        <PackageReference Include="Microsoft.Extensions.Options" Version="8.0.0" />
        <PackageReference Include="Volte.Features" Version="1.8.0" />
    </ItemGroup>

</Project>
